#foreach($cluster in $clusters)
<div class="box">
	<div class="box-header"><h3 class="box-title">$!{cluster.name}</h3></div>
	<div class="box-body no-padding">
		<table class="table table-condensed table-hover">
			<tbody>
				#foreach($node in $cluster.nodes)
				<tr><td><a href="/monitor/$!{node.host}:$!{node.port}">$!{node.host}:$!{node.port}</a></td></tr>
				#end
			</tbody>
		</table>
	</div>
</div>
#end
